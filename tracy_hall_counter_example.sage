vertices = [int(i) for i in vertices.split()]

Q = graphs.CubeGraph(12)
loopsided = Q.subgraph([Q.vertices()[i] for i in vertices])

I = igraph.Graph()
I.add_vertices(loopsided.vertices())
edges = [(e[0], e[1]) for e in loopsided.edges()]
I.add_edges(edges)

lay = I.layout_fruchterman_reingold()
a = igraph.plot(I, layout=lay, vertex_size=30,  bbox=(3000, 3000))
a.save("tracy_hall_graph.png")
